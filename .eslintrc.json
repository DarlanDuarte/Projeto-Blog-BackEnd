{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:prettier/recommended", "standard-with-typescript"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier", "react-hooks", "@typescript-eslint", "import"],
  "rules": {
    "semi": "off",
    "no-console": "off",
    "no-unused-vars": "warn",
    "linebreak-style": ["error", "unix"],
    "prettier/prettier": ["error", {}, { "endOfLine": "auto" }]
  }
}
